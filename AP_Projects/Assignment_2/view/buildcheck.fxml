    <Text fx:id="noMotherboard" visible="false" text="The build is missing a motherboard."/>
    <Text fx:id="noRAM" visible="false" text="The build is missing RAM."/>
    <Text fx:id="noCase" visible="false" text="The build is missing a case."/>
